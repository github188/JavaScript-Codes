/* 全局 */
#board-keys table td.bg-red {
	background-color: #FC0A0A;
}

@font-face {
	font-family: 'DigifaceWide';
	src: url('DigifaceWide.eot');
	font-weight: 100;
	font-style: normal;
}
@font-face {
	font-family: 'DigifaceWide';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('DigifaceWide.ttf') format('truetype');
	font-weight: 100;
	font-style: normal;
}


/* 0. 主面板 --------- start ----------- */
#main-board {
	width: 750rpx;
	height: 1334rpx;
	background-color: #C5C5C5;
	border-radius: 25rpx;
	box-shadow: 0 4rpx 8rpx 0 rgba(0, 0, 0, 0.2), 0 6rpx 20rpx 0 rgba(0, 0, 0, 0.19);
}

/* 1. 计算器名称 */
#board-title {
	width: 100%;
	height: 100rpx;
	/*background-color: #ccccff;*/
	text-align: center;
	letter-spacing: 4rpx;
	/* font-size/line-height font-family*/
	font: 60rpx/10r0px "Comic Sans MS", "微软雅黑";
	color: #4723AB;
}

/* 2. 结果屏显区 */
#board-result {
	width: 90%;
	height: 160rpx;
	background-color: #cce5ff;
	text-align: right;
	margin: 20rpx auto;
	border-radius: 16rpx;
	border: 4rpx solid #EAEAEA;
	box-shadow: 0 8rpx 16rpx 0 rgba(0, 0, 0, 0.2);
}

/* 显示结果屏显区域，up显示数字，down显示表达式 */
#result-up {
	width: 100%;
	height: 100rpx;
	position: relative;
}

/* 日期显示 */
#result-date {
	float: left;
	width: 100%;
	height: 20rpx;
	font-size: 28rpx;
	color: #857F7F;
	text-align: left;
	text-indent: 20rpx;
}

#result-up span {
	position: absolute;
	width: 100%;
	right: 0;
	bottom: 0;
	overflow: hidden;
}

#result-down {
	width: 100%;
	height: 60rpx;
	line-height: 60rpx;
	font-size: 40rpx;
	color: #857F7F;
	overflow: hidden;
}

/* 3. 按键区 */
#board-keys {
	width: 700rpx;
	height: 560rpx;
	/*background-color: #E8A4A4;*/
	margin-left: 25rpx;
	margin-bottom: 40rpx;
	color: #FFFFFF;
}

.keys-line {
	width: 100%;
	height:70rpx;
	text-align: center;
}

.keys-cell {
	width: 160rpx;
	padding: 15rpx;
	float: left;
}

#board-keys table td {
	width: 70rpx;
	height: 70rpx;
	background-color: #2B2828;
	/*border: 1px solid #2C2929; */
	border-radius: 16rpx;
	box-shadow: #2B2828, 0 8rpx 16rpx 0 rgba(0, 0, 0, 0.2), 0 12rpx 40rpx 0 rgba(0, 0, 0, 0.19);
}

#board-keys table td:hover {
	cursor: pointer;
}

/* 主面板 --------- end ----------- */